    "webpack": ">=5.0.0"
  },
  "overrides": {
    "vue-cli-plugin-electron-builder": {
      "electron-builder": "$electron-builder"
    },
    "parse-asn1": {
      "pbkdf2": "3.0.3"
    },
    "cache-loader": {
      "webpack": "$webpack"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
